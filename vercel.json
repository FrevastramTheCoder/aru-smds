{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/auth/:path*",
      "destination": "https://backend-sp9b.onrender.com/api/v1/auth/:path*"
    },
    {
      "source": "/api/spatial/:path*",
      "destination": "https://backend-sp9b.onrender.com/api/spatial/:path*"
    },
    {
      "source": "/auth/google/callback",
      "destination": "https://backend-sp9b.onrender.com/api/v1/auth/google/callback"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
